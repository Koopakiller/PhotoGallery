// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "node_modules/": true,
        "Styles/**/*.css": true,
        "App/**/*.js": true,
        "**/*.js.map": true,
        "obj": true,
        "bin": true
    },
    "beautify.options": {
        "indent_size": 4,
        "preserve_newlines": true,
        "end_with_newline": true
    }
}